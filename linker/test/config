MEMORY {
    INIT: start=0x00, size=0x20, type=ro;
    VEC: start=0x20, size=0x8, type=ro;
    ROM: start=0x28, size=0x1fd8, type=ro;
    RAM: start=0x2000, size=0xdfed, type=rw;
    SERIAL: start=0xffed, size=0x3, type=rw;
    ROM_2: start=0xfff0, size=0x10, type=ro;
}

SEGMENTS {
    SEG_INIT: load=INIT;
    SEG_VEC: load=VEC;
    SEG_PRG: load=ROM;
    SEG_GET_CHAR: load=ROM;
}
